let norn = require("../norn.js")

let {actions, ...store} = norn(
    {
        number: {
            initial() => 0
            $inc(num) => num + 1
            $dec(num) => num - 1
            $add(num, {value}) => num + value
        }
    }
    {
        "number.$add"(value) => {value}
    }
)

store.subscribe(
    (current) => console.log(current)
)

actions.$batch(
    ["number.$inc"]
    ["number.$add", 5]
    ["number.$dec"]
)
